## some mysql commands:
select * from zacks.portfolio_scan;
select * from zacks.portfolio_operations;
select id,portfolio,symbol,vol_percent,date_added,type,price,record_date,price_at_close
  from zacks.portfolio_operations ORDER BY record_date DESC, id ASC;
select * from zacks.portfolio_scan where record_date = CURDATE();
select * from zacks.portfolio_operations where record_date = CURDATE();
DELETE from zacks.portfolio_scan where record_date = CURDATE();
DELETE from zacks.portfolio_operations where record_date = CURDATE();

DELETE from zacks.portfolio_scan where record_date >= '2018-09-24';
DELETE from zacks.portfolio_operations where record_date >= '2018-09-24';


## some useful combo
source ./src/main/resources/remotedb.properties
mysql --user=${user} --password=${password} --host=${host} \
  -e "select * from zacks.portfolio_operations where record_date = CURDATE()"

source ./src/main/resources/database.properties
mysql --user=${user} --password=${password} --host=${host} \
  -e "select * from zacks.portfolio_operations where record_date = (SELECT MAX(record_date) FROM zacks.portfolio_operations);"
mysql --user=${user} --password=${password} --host=${host} \
  -e "select * from zacks.portfolio_scan where record_date = (SELECT MAX(record_date) FROM zacks.portfolio_scan) ORDER BY portfolio AND symbol"

